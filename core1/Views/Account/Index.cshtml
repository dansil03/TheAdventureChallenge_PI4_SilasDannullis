@{
}

<table class="table table-dark table-striped table-hover">
    <thead>
        <tr>
            <th>Username</th>
            <th>Password</th>
            <th>Email</th>
        </tr>
    </thead>

    <tbody>
        @{
            foreach(User user in ViewBag.users)
            {
                <tr>
                    <td>
                        @user.UserName
                    </td>
                    <td>
                        @user.Password
                    </td>
                    <td>
                        @user.Email
                    </td>

                </tr>
            }
        }
    </tbody>
</table>
    
